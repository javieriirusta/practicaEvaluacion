function verNota(nota) {
    if (nota <= 4) {
        return "LIBRE"
    } else if (nota < 7 && nota > 4) {
        return "REGULAR"
    } else if (nota > 6 && nota <= 10){
        return "PROMOCIONADO"
    }
    else if (nota > 10) {
        return "ERROR"
    }
}
////EJEMPLO////
console.log(verNota(11))
////SALIDA: "ERROR"////


